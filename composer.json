{
    "name": "zerai/oberdan8",
    "type": "project",
    "description": "booking application",
    "license": "proprietary",
    "require": {
        "php": ">=7.4 || 8.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-xsl": "*",
        "composer/package-versions-deprecated": "1.11.99.5",
        "corley/maintenance-bundle": "^0.4",
        "doctrine/annotations": "^1.13",
        "doctrine/doctrine-bundle": "^2.7",
        "doctrine/doctrine-migrations-bundle": "^3.1",
        "doctrine/orm": "^2.14",
        "knplabs/knp-paginator-bundle": "^5.9",
        "league/commonmark": "^2.3",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpstan/phpdoc-parser": "^1.15",
        "prolic/fpp": "dev-master",
        "psr/log": "^1.1",
        "ramsey/uuid": "^4.2",
        "ramsey/uuid-doctrine": "^2.0",
        "sensio/framework-extra-bundle": "^6.2",
        "sentry/sentry-symfony": "^4.5",
        "symfony/apache-pack": "^1.0",
        "symfony/console": "4.4.*",
        "symfony/dotenv": "4.4.*",
        "symfony/flex": "^1.19.3",
        "symfony/form": "4.4.*",
        "symfony/framework-bundle": "4.4.*",
        "symfony/google-mailer": "4.4.*",
        "symfony/mailer": "4.4.*",
        "symfony/monolog-bundle": "^3.7",
        "symfony/property-access": "4.4.*",
        "symfony/property-info": "4.4.*",
        "symfony/proxy-manager-bridge": "4.4.*",
        "symfony/security-bundle": "4.4.*",
        "symfony/serializer": "4.4.*",
        "symfony/twig-bundle": "^4.4",
        "symfony/validator": "4.4.*",
        "symfony/webpack-encore-bundle": "^1.16",
        "symfony/yaml": "4.4.*",
        "symfonycasts/verify-email-bundle": "^1.10",
        "twig/cssinliner-extra": "^3.4",
        "twig/extra-bundle": "^3.3",
        "twig/inky-extra": "^3.10",
        "twig/markdown-extra": "^3.4",
        "twig/string-extra": "^3.4",
        "twig/twig": "^3.4.3"
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php56": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php71": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "require-dev": {
        "bamarni/composer-bin-plugin": "^1.8",
        "dama/doctrine-test-bundle": "^6.7",
        "dbrekelmans/bdi": "^0.3.0",
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "symfony/browser-kit": "4.4.*",
        "symfony/css-selector": "4.4.*",
        "symfony/maker-bundle": "^1.39",
        "symfony/panther": "^1.1",
        "symfony/phpunit-bridge": "^6.2",
        "symfony/stopwatch": "^4.4",
        "symfony/var-dumper": "4.4.*",
        "symfony/web-profiler-bundle": "^4.4",
        "zenstruck/foundry": "^1.23"
    },
    "config": {
        "allow-plugins": {
            "bamarni/composer-bin-plugin": true,
            "symfony/flex": true
        },
        "platform": {
            "php": "7.4"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "bamarni-bin": {
            "bin-links": true,
            "forward-command": false,
            "target-directory": "tools"
        },
        "symfony": {
            "allow-contrib": false,
            "require": "4.4.*"
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Booking\\": "core/booking/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/",
            "Booking\\Tests\\": "core/booking/tests/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
