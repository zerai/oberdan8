<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <meta name="robots" content="noindex, nofollow">
{#        <link rel="shortcut icon" href="/images/favicon.jpg" />#}
        <link rel="shortcut icon" href="{{ asset('build/images/backoffice/favicon.jpg') }}" />

        <style>
            .bd-placeholder-img {
                font-size: 1.125rem;
                text-anchor: middle;
                -webkit-user-select: none;
                -moz-user-select: none;
                -ms-user-select: none;
                user-select: none;
            }

            @media (min-width: 768px) {
                .bd-placeholder-img-lg {
                    font-size: 3.5rem;
                }
            }
        </style>


        <title>{% block title %}Administration - Oberdan 8 {% endblock %}</title>
        {# Run `composer require symfony/webpack-encore-bundle`
           and uncomment the following Encore helpers to start using Symfony UX #}
        {% block stylesheets %}
            {{ encore_entry_link_tags('backoffice') }}
        {% endblock %}

        {% block javascripts %}
{#            <script src="https://code.jquery.com/jquery-3.5.1.slim.min.js" integrity="sha384-DfXdz2htPH0lsSSs5nCTpuj/zy4C+OGpamoFVy38MVBnE+IbbVYUew+OrCXaRkfj" crossorigin="anonymous"></script>#}
            {{ encore_entry_script_tags('backoffice') }}
        {% endblock %}
    </head>
    <body>
        {% block header %}
            {% include 'backoffice/static/_header-navbar.html.twig' %}
        {% endblock %}
        <div class="container-fluid">
            <div class="row">
                {% block sidebar %}
                    {% include 'backoffice/static/_sidebar.html.twig' %}
                {% endblock %}
                <main role="main" class="col-md-9 ml-sm-auto col-lg-10 px-md-4">
                    {% block body %}{% endblock %}
